####### Install

install_target: first FORCE
	@test -d $(INSTALL_ROOT)/usr/bin || mkdir -p $(INSTALL_ROOT)/usr/bin
	$(QINSTALL_PROGRAM) $(QMAKE_TARGET) $(INSTALL_ROOT)/usr/bin/$(QMAKE_TARGET)
	-$(STRIP) $(INSTALL_ROOT)/usr/bin/$(QMAKE_TARGET)

uninstall_target: FORCE
	-$(DEL_FILE) $(INSTALL_ROOT)/usr/bin/$(QMAKE_TARGET)
	-$(DEL_DIR) $(INSTALL_ROOT)/usr/bin/ 


install_desktop-file: first FORCE
	@test -d $(INSTALL_ROOT)/usr/share/applications || mkdir -p $(INSTALL_ROOT)/usr/share/applications
	$(QINSTALL_PROGRAM) /home/vigil/qjournalctl/ui/qjournalctl.desktop $(INSTALL_ROOT)/usr/share/applications/qjournalctl.desktop
	-echo $(INSTALL_ROOT)/usr/share/applications/qjournalctl.desktop

uninstall_desktop-file: FORCE
	-$(DEL_FILE) -r $(INSTALL_ROOT)/usr/share/applications/qjournalctl.desktop
	-$(DEL_DIR) $(INSTALL_ROOT)/usr/share/applications/ 


install_desktop-icon: first FORCE
	@test -d $(INSTALL_ROOT)/usr/share/pixmaps || mkdir -p $(INSTALL_ROOT)/usr/share/pixmaps
	$(QINSTALL) /home/vigil/qjournalctl/ui/qjournalctl.png $(INSTALL_ROOT)/usr/share/pixmaps/qjournalctl.png

uninstall_desktop-icon: FORCE
	-$(DEL_FILE) -r $(INSTALL_ROOT)/usr/share/pixmaps/qjournalctl.png
	-$(DEL_DIR) $(INSTALL_ROOT)/usr/share/pixmaps/ 


install_metainfo-file: first FORCE
	@test -d $(INSTALL_ROOT)/usr/share/metainfo || mkdir -p $(INSTALL_ROOT)/usr/share/metainfo
	$(QINSTALL) /home/vigil/qjournalctl/qjournalctl.appdata.xml $(INSTALL_ROOT)/usr/share/metainfo/qjournalctl.appdata.xml

uninstall_metainfo-file: FORCE
	-$(DEL_FILE) -r $(INSTALL_ROOT)/usr/share/metainfo/qjournalctl.appdata.xml
	-$(DEL_DIR) $(INSTALL_ROOT)/usr/share/metainfo/ 


install: install_target install_desktop-file install_desktop-icon install_metainfo-file  FORCE

uninstall: uninstall_target uninstall_desktop-file uninstall_desktop-icon uninstall_metainfo-file  FORCE

FORCE:

